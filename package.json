    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-terser": "2.0.2",
    "tape": "^4.9.1",
    "testling": "^1.7.1"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noaoh/minsky.git"
  },
